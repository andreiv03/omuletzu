{
	"name": "omuletzu",
	"private": true,
	"scripts": {
		"build": "rimraf dist && swc src -d dist && shx mv dist/src/* dist && rimraf dist/src",
		"start": "node dist/index.js",
		"dev": "nodemon --watch src --ext ts --exec \"npm run build && npm run start\"",
		"lint": "eslint . --ext .ts && tsc --noEmit",
		"deploy": "npm run build && node -r dotenv-flow/config ./dist/utils/deploy-commands.js"
	},
	"dependencies": {
		"axios": "^1.9.0",
		"discord.js": "^14.19.3",
		"dotenv-flow": "^4.1.0"
	},
	"devDependencies": {
		"@swc/cli": "^0.7.7",
		"@swc/core": "^1.11.29",
		"@types/node": "^22.15.29",
		"@typescript-eslint/eslint-plugin": "^8.33.0",
		"@typescript-eslint/parser": "^8.33.0",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-security": "^3.0.1",
		"nodemon": "^3.1.10",
		"prettier": "^3.5.3",
		"rimraf": "^6.0.1",
		"shx": "^0.4.0",
		"typescript": "^5.8.3"
	}
}
